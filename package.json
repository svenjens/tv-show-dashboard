{
  "name": "bingelist",
  "private": true,
  "version": "1.12.0",
  "type": "module",
  "description": "BingeList - Your ultimate TV show discovery and tracking platform",
  "homepage": "https://bingelist.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:a11y": "pa11y-ci",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "generate:branding": "node scripts/generate-branding.js",
    "optimize:images": "node scripts/optimize-images.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "compare:bundle": "node scripts/compare-bundle.js"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.2.0",
    "@vueuse/motion": "^3.0.3",
    "axios": "^1.13.2",
    "dompurify": "^3.3.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "openai": "^6.9.0",
    "pa11y-ci": "^4.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9",
    "vue-tsc": "^3.1.3"
  }
}
